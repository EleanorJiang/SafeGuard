<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.parse.starter">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".StarterApplication"
        android:allowBackup="true"
        android:icon="@drawable/new_icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.parse.APPLICATION_ID"
            android:value="@string/parse_app_id" />
        <meta-data
            android:name="com.parse.CLIENT_KEY"
            android:value="@string/parse_client_key" />

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UserListActivity" />
        <activity android:name=".UserFeedActivity" />
        <activity
            android:name=".PatientHome"
            android:label="@string/title_activity_patient_home" />
        <activity android:name=".PatientChat" />
        <activity android:name=".PatientMessage" />
        <activity android:name=".PatientSetting" />
        <activity android:name=".DoctorChat" />
        <activity android:name=".DoctorSetting" />
        <activity android:name=".PatientMeasure" />
        <activity android:name=".PatientAdd" />
        <activity android:name=".PatientAdd1" />
        <activity android:name=".PatientAdd2" />
        <activity android:name=".Settingpassword" />
        <activity android:name=".Changedoctor" />
        <activity android:name=".PatientSettingDetail" />
        <activity android:name=".settingemail" />
        <activity android:name=".DoctorAddPatient" />
        <activity android:name=".Doctor_PatientInfo" />
        <activity android:name=".sendNewMassege" />
        <activity android:name=".DoctorSettingDetail"></activity>
    </application>

</manifest>